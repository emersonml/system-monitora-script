{
    "files.exclude": {
        "node_modules": true,
        ".next": true,
        "public/sw.js": true,
        "public/workbox-*.js": true,
        "next-env.d.ts": true
    },

    "editor.tabSize": 4,
    "editor.formatOnSave": true,
    "editor.insertSpaces": true,
    "editor.wordWrapColumn": 120,
    "editor.rulers": [120],
    "editor.codeActionsOnSave": {
        "source.fixAll": "explicit"
    },
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "[html]": { "editor.defaultFormatter": "mohd-akram.vscode-html-format" },
    "[svg]": { "editor.defaultFormatter": "jock.svg" },
    "[prisma]": {
        "editor.defaultFormatter": "Prisma.prisma"
    },
    "cSpell.words": ["conclusao", "observacao", "pixlab", "Recokigition", "sisgama", "vinculos", "Vinculos"]
}
